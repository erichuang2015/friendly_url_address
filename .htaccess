RewriteEngine on
RewriteRule ^sign-up$ pages/sign-up/$1
RewriteRule ^sign-up/$ pages/sign-up/$1
RewriteRule ^profile/([a-zA-Z0-9-]+)$ pages/profile/index.php?profile=$1
RewriteRule ^profile/([a-zA-Z0-9-]+)/$ pages/profile/index.php?profile=$1
RewriteRule ^category/([a-zA-Z0-9-]+)$ pages/category/index.php?category=$1
RewriteRule ^category/([a-zA-Z0-9-]+)/$ pages/category/index.php?category=$1
RewriteRule ^category/([a-zA-Z0-9-]+)/([a-zA-Z0-9]+)$ pages/category/index.php?category=$1&episode=$2
RewriteRule ^category/([a-zA-Z0-9-]+)/([a-zA-Z0-9]+)/$ pages/category/index.php?category=$1&episode=$2
RewriteRule ^edit/image/([a-zA-Z0-9-]+)$ pages/edit/image/index.php?image=$1
RewriteRule ^edit/image/([a-zA-Z0-9-]+)/$ pages/edit/image/index.php?image=$1